   "source": [
    "# Load the original dataset\n",
    "spotify = pd.read_csv(\"spotify_dataset.csv\",\n",
    "                   names = [\"user_id\", \"artistname\", \"trackname\", \"playlistname\"], \n",
    "                   sep = '\\\",\\\"', \n",
    "                   engine = 'python',\n",
    "                   skiprows = 1).dropna().reset_index(drop=True)\n",
    "spotify"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "2fbb9677",
   "metadata": {},
   "source": [
    "### Data Cleaning"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "518280cf",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fix the artist names removing characters\n",
    "artistname = []\n",
    "for i in range(len(spotify)):\n",
    "    artistname.append(unicodedata.normalize('NFKD',spotify[\"artistname\"][i]).encode(\"ascii\",\"ignore\").decode('ascii'))\n",
    "\n",
    "spotify[\"artistname\"] = artistname"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "b7335328",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fix the strings for dataset, removing quotation marks\n",
    "spotify['user_id'] = spotify['user_id'].str.replace('\"', '')\n",
    "spotify['artistname'] = spotify['artistname'].str.replace('\"', '')\n",
    "spotify['trackname'] = spotify['trackname'].str.replace('\"', '')\n",
    "spotify['playlistname'] = spotify['playlistname'].str.replace('\"', '')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "96d799da",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fix the empty string and string nan, removing frmo dataset\n",
    "spotify = spotify[(spotify['user_id'] != \"\")]\n",
    "spotify = spotify[(spotify['artistname'] != \"\")]\n",
    "spotify = spotify[(spotify['trackname'] != \"\")]\n",
    "spotify = spotify[(spotify['playlistname'] != \"\")]\n",
    "\n",
    "spotify = spotify[(spotify['user_id'] != \"null\")]\n",
    "spotify = spotify[(spotify['artistname'] != \"null\")]\n",
    "spotify = spotify[(spotify['trackname'] != \"null\")]\n",
    "spotify = spotify[(spotify['playlistname'] != \"null\")]"
